{
  "builds": [{ "src": "app/main.py", "use": "@vercel/python" }],
  "routes": [{ "src": "/(.*)", "dest": "app/main.py" }],
  "crons": [
    {
      "path": "/api/cron_event_rollup",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron_revoke_enforcer", 
      "schedule": "*/1 * * * *"
    }
  ]
}